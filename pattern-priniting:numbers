#include<iostream>
using namespace std;
int main(){
    int n;
    cin>>n; 
    for(int i=1;i<=n;i++){
        int num=1;
        for(int k=1;k<=n-i;k++){
            cout<<" ";
        }
        for(int j=1;j<=i;j++){
            cout<<num;
            num++;
        }
        for(int z=i-1;z>=1;z--){
            cout<<z;
        }
        
        cout<<"\n";
        
    }
}

